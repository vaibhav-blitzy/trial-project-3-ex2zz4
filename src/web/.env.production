VITE_APP_ENV=production
VITE_APP_TITLE=Task Management System
VITE_LOG_LEVEL=error

# API Configuration
VITE_API_URL=https://api.taskmanagement.com/api/v1
VITE_WS_URL=wss://api.taskmanagement.com/ws
VITE_API_TIMEOUT=10000
VITE_ENABLE_MOCK_API=false

# Authentication Configuration
VITE_GOOGLE_CLIENT_ID=${GOOGLE_CLIENT_ID}
VITE_GOOGLE_REDIRECT_URI=https://taskmanagement.com/auth/google/callback
VITE_MS_CLIENT_ID=${MS_CLIENT_ID}
VITE_MS_REDIRECT_URI=https://taskmanagement.com/auth/microsoft/callback

# Token Management
VITE_AUTH_TOKEN_EXPIRY=3600
VITE_REFRESH_TOKEN_EXPIRY=86400

# Security Settings
VITE_CORS_ENABLED=true
VITE_CORS_ORIGINS=https://taskmanagement.com
VITE_CSP_ENABLED=true
VITE_SECURE_COOKIES=true
VITE_RATE_LIMIT_REQUESTS=100
VITE_RATE_LIMIT_INTERVAL=60000

# Development Tools
VITE_ENABLE_DEBUG_TOOLS=false

# Monitoring and Observability
VITE_ERROR_TRACKING_ENABLED=true
VITE_PERFORMANCE_MONITORING_ENABLED=true
VITE_REQUEST_TRACING_ENABLED=true