# Application Environment
VITE_APP_ENV=test
VITE_APP_TITLE="Task Management System - Test"
VITE_LOG_LEVEL=debug

# API Configuration
VITE_API_URL=http://localhost:3001/api/v1
VITE_WS_URL=ws://localhost:3001
VITE_API_TIMEOUT=5000
VITE_ENABLE_MOCK_API=true
VITE_MOCK_API_DELAY=100

# Authentication Configuration
VITE_GOOGLE_CLIENT_ID=test-google-client-id
VITE_GOOGLE_REDIRECT_URI=http://localhost:5174/auth/google/callback
VITE_MS_CLIENT_ID=test-microsoft-client-id
VITE_MS_REDIRECT_URI=http://localhost:5174/auth/microsoft/callback
VITE_AUTH_TOKEN_EXPIRY=3600
VITE_REFRESH_TOKEN_EXPIRY=86400

# Testing Configuration
VITE_ENABLE_DEBUG_TOOLS=true
VITE_TEST_COVERAGE_THRESHOLD=80
VITE_TEST_TIMEOUT=10000
VITE_TEST_DATA_CLEANUP=true
VITE_TEST_DATABASE_NAME=task_mgmt_test
VITE_ENABLE_TEST_LOGGING=true
VITE_TEST_PERFORMANCE_MONITORING=true